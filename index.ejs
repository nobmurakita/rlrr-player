<!DOCTYPE html>
<html lang="ja">
  <head>
    <title>RLRR Player</title>
  </head>
  <body>
    <h2><%= songsDir %></h2>
    <ul>
        <% for (const song of songs) { %>
            <li>
                <a href="<%= song.playerLink %>" target="_blank">
                    <%= song.artist %> - <%= song.title %> [<%= song.level %>]
                </a>
            </li>
        <% } %>
    </ul>
  </body>
</html>
